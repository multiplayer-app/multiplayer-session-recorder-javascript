{
  "name": "@multiplayer-app/session-recorder-browser",
  "version": "1.3.13",
  "description": "Multiplayer Fullstack Session Recorder for Browser",
  "author": {
    "name": "Multiplayer Software, Inc.",
    "url": "https://www.multiplayer.app"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/multiplayer-app/multiplayer-session-recorder-javascript/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/multiplayer-app/multiplayer-session-recorder-javascript.git"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.umd.js"
    },
    "./browser": "./dist/browser/index.js",
    "./exporters": "./dist/exporters/index.js"
  },
  "engines": {
    "node": ">=18",
    "npm": ">=8"
  },
  "keywords": [
    "multiplayer",
    "debugger",
    "platform",
    "platform debugger",
    "session recorder",
    "otlp",
    "fullstack session recorder",
    "browser"
  ],
  "scripts": {
    "lint": "eslint src/**/*.ts",
    "preversion": "npm run lint",
    "postversion:skip": "git push && git push --tags",
    "build": "rm -rf dist tsconfig.tsbuildinfo && tsc && webpack",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "browserify": "17.0.1",
    "clean-webpack-plugin": "4.0.0",
    "css-loader": "6.0.0",
    "css-minimizer-webpack-plugin": "7.0.0",
    "eslint": "8.48.0",
    "mini-css-extract-plugin": "2.9.1",
    "sass": "^1.79.4",
    "sass-loader": "16.0.2",
    "style-loader": "3.0.0",
    "terser-webpack-plugin": "5.3.10",
    "ts-loader": "9.5.1",
    "typescript": "5.7.3",
    "webpack": "5.95.0",
    "webpack-bundle-analyzer": "4.10.2",
    "webpack-cli": "5.1.4"
  },
  "dependencies": {
    "@multiplayer-app/session-recorder-common": "1.3.13",
    "@opentelemetry/auto-instrumentations-web": "0.49.0",
    "@opentelemetry/context-zone": "2.0.1",
    "@opentelemetry/core": "2.0.1",
    "@opentelemetry/exporter-trace-otlp-http": "0.203.0",
    "@opentelemetry/instrumentation": "0.203.0",
    "@opentelemetry/otlp-exporter-base": "0.203.0",
    "@opentelemetry/otlp-transformer": "0.203.0",
    "@opentelemetry/resources": "2.0.1",
    "@opentelemetry/sdk-trace-base": "2.0.1",
    "@opentelemetry/sdk-trace-web": "2.0.1",
    "@opentelemetry/semantic-conventions": "1.36.0",
    "@rrweb/packer": "2.0.0-alpha.15",
    "@rrweb/rrweb-plugin-console-record": "2.0.0-alpha.15",
    "dompurify": "3.2.5",
    "lib0": "0.2.82",
    "rrweb": "2.0.0-alpha.15",
    "socket.io-client": "4.7.5",
    "to-json-schema": "^0.2.5"
  },
  "peerDependencies": {
    "@opentelemetry/api": "^1.9.0"
  }
}
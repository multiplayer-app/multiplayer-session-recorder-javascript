    "@opentelemetry/otlp-transformer": "^0.56.0",
    "@opentelemetry/sdk-trace-base": "^1.29.0",
    "axios": "^1.10.0",
    "to-json-schema": "^0.2.5"
  },
  "devDependencies": {
    "@types/node": "24.0.12",
    "typescript": "5.8.3"

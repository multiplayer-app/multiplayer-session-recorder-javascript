{
  "name": "@multiplayer-app/session-recorder-node",
  "version": "0.0.8",
  "description": "Multiplayer Fullstack Session Recorder for Node.js",
  "author": {
    "name": "Multiplayer Software, Inc.",
    "url": "https://www.multiplayer.app"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "engines": {
    "node": ">=18",
    "npm": ">=8"
  },
  "keywords": [
    "multiplayer",

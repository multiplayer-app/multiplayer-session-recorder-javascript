{
  "name": "cli-example",
  "version": "1.0.0",
  "description": "CLI demo",
  "author": {
    "name": "Multiplayer Software, Inc.",
    "url": "https://www.multiplayer.app"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "engines": {
    "node": ">=18",
    "npm": ">=8"
  },
  "keywords": [
    "multiplayer",
